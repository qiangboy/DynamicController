{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "HttpMethodInfo": [
        {
            "MethodKey": "Get",
            "MethodValues": [ "GET", "QUERY" ]
        },
        {
            "MethodKey": "Post",
            "MethodValues": [ "CREATE", "SAVE", "INSERT", "ADD" ]
        },
        {
            "MethodKey": "Put",
            "MethodValues": [ "UPDATE", "EDIT" ]
        },
        {
            "MethodKey": "Delete",
            "MethodValues": [ "Delete", "REMOVE" ]
        }
    ]
}
